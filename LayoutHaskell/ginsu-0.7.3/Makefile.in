	  case '$(am__configure_deps)' in \
	    *$$dep*) \
	      echo ' cd $(srcdir) && $(AUTOMAKE) --foreign  --ignore-deps'; \
	      cd $(srcdir) && $(AUTOMAKE) --foreign  --ignore-deps \
		&& exit 0; \
	      exit 1;; \
