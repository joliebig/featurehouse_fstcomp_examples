module check

rules

  constraint-error:
  	Eval(e) -> (e, $[Erwarteter Typ für eval(): Int; Erhaltener Typ: String])
  	where
  		<typeof> e => Type("String")
  
  constraint-error:
  	Eval(e) -> (e, $[Erwarteter Typ für eval(): Int; Erhaltener Typ: undefiniert])
  	where
  		not(<typeof> e)
