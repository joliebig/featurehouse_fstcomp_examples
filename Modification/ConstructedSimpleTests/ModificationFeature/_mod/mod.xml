<?xml version="1.0" encoding="UTF-8"?>
<modificationComposition>
	<!-- testing input types / java mods -->
	<modification><!-- introduction -->
		<type>introduction</type>
		<traversal>..*:Feature</traversal>
		<content>
			<parsed>
				<externLink>Foo.java</externLink>
			</parsed>
		</content>
	</modification>
	<modification>
		<type>introduction</type>
		<traversal>..*:ClassDeclaration</traversal>
		<content>
			<parsed>
				<plainText>
					<text>public void foo(){}</text>
					<tType>java.method</tType>
				</plainText>
			</parsed>
		</content>
	</modification>
	<modification><!-- superimposition -->
		<type>superimposition</type>
		<traversal>..foo*:*</traversal>
		<content>
			<parsed>
				<plainText>
					<text>public void foo(){System.out.println("b√§m");}</text>
					<tType>java.method</tType>
				</plainText>
			</parsed>
		</content>
	</modification>
	<modification>
		<type>superimposition</type>
		<traversal>..*print*:*</traversal>
		<content>
			<parsed>
				<externLink>Bar.java</externLink>
				<cTraversal>..*print*:*</cTraversal>
			</parsed>
		</content>
	</modification>
	<modification><!-- custom content / introduction on more than one location -->
		<type>introduction</type>
		<traversal>..*:CompilationUnit</traversal>
		<content>
			<custom>
				<nodeType>ImportDeclaration</nodeType>
				<name>java.util.List{ImportPackage}
				</name>
				<body>import java.util.List;</body>
				<prefix></prefix>
				<compositionMechanism>compose:Replacement
				</compositionMechanism>
			</custom>
		</content>
	</modification>
	<!--C mods-->
	<modification><!-- introduction -->
		<type>introduction</type>
		<traversal>..Test*:*..*:Sequence_CodeUnit_TopLevel
		</traversal>
		<content>
			<parsed>
				<plainText>
					<text>void foo( void ) { bar(); }</text>
					<tType>c.function</tType>
				</plainText>
			</parsed>
		</content>
	</modification>
	<modification><!-- superimposition -->
		<type>superimposition</type>
		<traversal>..Test*:*..foo*:Func
		</traversal>
		<content>
			<parsed>
				<plainText>
					<text>void foo( void ) { original(); foobar(); }</text>
					<tType>c.function</tType>
				</plainText>
			</parsed>
		</content>
	</modification>
	<!-- haskell -->
	<modification><!-- introduction -->
		<type>introduction</type>
		<traversal>..HaskellTest*:*..*:definitions
		</traversal>
		<content>
			<parsed>
				<plainText>
					<text>plus a b = a + b</text>
					<tType>haskell.definition</tType>
				</plainText>
			</parsed>
		</content>
	</modification>
</modificationComposition>