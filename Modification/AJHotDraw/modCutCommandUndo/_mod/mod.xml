	<modification>
		<type>javaMethodBodyOverriding</type>
		<traversal>..CutCommand*:ClassDeclaration.*execute*:MethodDecl</traversal>
		<content>
			<parsed>
				<plainText>
					<text>
						{setUndoActivity(createUndoActivity());
						getUndoActivity().setAffectedFigures(collectAffectedFigures());
						UndoActivity ua = (UndoActivity) acommand.getUndoActivity();
						ua.setSelectedFigures(acommand.view().selection());
						original();}</text>
					<tType>java.methodBody</tType>
				</plainText>
			</parsed>
		</content>
	</modification>
	<modification>
		<type>introduction</type>
		<traversal>..CutCommand*:ClassDeclaration</traversal>
		<content>
			<parsed>
				<plainText>
					<text>
						public Undoable createUndoActivity() {
						return new UndoActivity(this);
					}
					</text>
					<tType>java.method</tType>
				</plainText>
			</parsed>
		</content>
	</modification>
	<modification>
		<type>introduction</type>
		<traversal>..CutCommand*:ClassDeclaration</traversal>
		<content>
			<parsed>
				<externLink>AuxClass.java</externLink>
				<cTraversal>..*:InnerClass*</cTraversal>
